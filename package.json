{
  "name": "vdf-bana-ozel",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.12.4",
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "payment": "^2.3.0",
    "react": "^16.7.0",
    "react-credit-card-primitives": "^1.5.1",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "react-text-mask": "^5.4.3",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "text-mask-addons": "^3.8.0"
  },
  "scripts": {
    "build-css": "lessc src/assets/main.less ./src/assets/main.css --js",
    "watch-css": "less-watch-compiler src/ ./src --enable-js --main-file ./assets/main.less",
    "start-js": "react-scripts start",
    "start": "sh -ac 'npm-run-all -p watch-css start-js'",
    "build-js": "react-scripts build",
    "build": "sh -ac 'npm-run-all build-css build-js'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "less": "^3.9.0",
    "less-watch-compiler": "^1.13.0",
    "npm-run-all": "^4.1.5"
  }
}
